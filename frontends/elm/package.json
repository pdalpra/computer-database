{
  "name": "our-elm-spa-app",
  "version": "1.0.0",
  "description": "A project created with elm-spa",
  "scripts": {
    "start": "npm install && npm run build:dev && npm run dev",
    "test": "elm-test",
    "test:watch": "elm-test --watch",
    "coverage": "elm-coverage",
    "build": "run-s build:elm-spa build:elm minify",
    "build:dev": "run-s build:elm-spa build:dev:elm",
    "dev": "run-p dev:elm-spa dev:elm",
    "build:elm": "elm make src/Main.elm --optimize --output=public/dist/elm.compiled.js",
    "build:dev:elm": "elm make src/Main.elm --debug --output=public/dist/elm.compiled.js || true",
    "build:elm-spa": "elm-spa build .",
    "dev:elm": "elm-live src/Main.elm -u -d public -- --debug --output=public/dist/elm.compiled.js",
    "dev:elm-spa": "chokidar src/Pages -c \"elm-spa build .\"",
    "minify": "run-s uglify:compress uglify:mangle",
    "uglify:compress": "uglifyjs public/dist/elm.compiled.js -o public/dist/elm.compiled.js -c 'pure_funcs=\"F2,F3,F4,F5,F6,F7,F8,F9,A2,A3,A4,A5,A6,A7,A8,A9\",pure_getters,keep_fargs=false,unsafe_comps,unsafe'",
    "uglify:mangle": "uglifyjs public/dist/elm.compiled.js -m -o public/dist/elm.compiled.js",
    "format": "elm-format --yes src/Pages/ src/Shared.elm review/src",
    "lint": "elm-review src/Pages/ src/Shared.elm review/src"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "chokidar-cli": "2.1.0",
    "elm": "0.19.1-3",
    "elm-coverage": "^0.3.0",
    "elm-format": "^0.8.3",
    "elm-live": "4.0.2",
    "elm-review": "^2.3.2",
    "elm-spa": "5.0.3",
    "elm-test": "0.19.1-revision2",
    "npm-run-all": "4.1.5",
    "uglify-js": "^3.10.4"
  },
  "dependencies": {}
}
